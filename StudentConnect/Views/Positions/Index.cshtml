@{
    ViewBag.Title = "Positions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div data-role="page" id="positions"  >

    @{ Html.RenderPartial("_Header", "Positions"); }

    <div data-role="content" data-theme="b" style="padding: 10px;">	
        @foreach (StudentConnect.Data.Position p in Model)
        { 
            <div>
                <span>@p.Title</span>
                &nbsp;
                <span>@p.Description</span>
            </div>
        }        
    </div>

    @{ Html.RenderPartial(@"_Footer"); }

</div>



